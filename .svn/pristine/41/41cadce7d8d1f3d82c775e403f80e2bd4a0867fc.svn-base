	}

	public void setRoleExp(int roleExp) {
		this.roleExp = roleExp;
	}

	public int getDropGroupId() {
		return dropGroupId;
	}

	public void setDropGroupId(int dropGroupId) {
		this.dropGroupId = dropGroupId;
	}

