	protected int sceneId;

	public void check() throws TemplateConfigException {
		// TODO Auto-generated method stub
	}

	public int getBattleNameId() {
		return battleNameId;
	}

	public void setBattleNameId(int battleNameId) {
		this.battleNameId = battleNameId;
	}

	public int getBattleDescId() {
		return battleDescId;
	}

	public void setBattleDescId(int battleDescId) {
		this.battleDescId = battleDescId;
	}

	public int getMonsterGroup1Id() {
		return monsterGroup1Id;
