option java_package = "com.doteyplay.game.message.proto";
option java_outer_classname = "NpcProBuf";
import "item.proto";
message PNpc{
   required int64 roleId=1;//玩家id
   required string npcName=2;//玩家名字
   required int32 level=3;//等级
   repeated int32 skillIdList=4;//技能列表
   required int32 quality=5;//品质
   required int32 star=6;//星级
   required int64 npcId=7;//id
   required int32 spriteId=8;//原型精灵id
   required int32 exp=9;//经验
   repeated PItem equipList=10;//装备列表
}

message PAddOrRemoveNpc{
   repeated PNpc npcs=1;
   required bool addOrRemove=2; //true为添加，false为删除
}