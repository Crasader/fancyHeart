package com.doteyplay.game.domain.gamebean;

public class ItemBean extends BaseBean
{
	private long roleId;
	private int itemId;
	private int itemNum;
	private long dressedSpriteId;

	public long getRoleId()
	{
		return roleId;
	}

	public void setRoleId(long roleId)
	{
		this.roleId = roleId;
	}

	public int getItemId()
	{
		return itemId;
	}

	public void setItemId(int itemId)
	{
		this.itemId = itemId;
	}

	public int getItemNum()
	{
		return itemNum;
	}

	public void setItemNum(int itemNum)
	{
		this.itemNum = itemNum;
	}

	public long getDressedSpriteId()
	{
		return dressedSpriteId;
	}

	public void setDressedSpriteId(long dressedSpriteId)
	{
		this.dressedSpriteId = dressedSpriteId;
	}
}
