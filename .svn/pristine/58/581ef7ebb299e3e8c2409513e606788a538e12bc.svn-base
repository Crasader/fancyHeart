option java_package = "com.doteyplay.game.message.proto";
option java_outer_classname = "ChatProBuf";


message PChatReq{
   required int32 type=1;//消息类型1功能2普通 
   required string msg=2;//消息
}
message PChatResp{
   optional string msg=1;//消息
}

message PChatUpdate{
   required int32 msgType=1;//0玩家消息1系统消息2工会消息3活动消息
   required string msgName=2;//玩家名字
   required int64 roleId=3;
   required string msg=4;//消息内容
}

message PChatTag{
   required int32 type=1;//1开启2关闭
}