package com.doteyplay.game.domain.common;

import java.util.ArrayList;
import java.util.List;

public class TextObject
{
	private List<String> replaceList = new ArrayList<String>();
	
	private TextGameObject gameObject;
	
	public TextObject(TextGameObject obj)
	{
		this.gameObject = obj; 
	}

	public List<String> getReplaceList()
	{
		return replaceList;
	}

	public void setReplaceList(List<String> replaceList)
	{
		this.replaceList = replaceList;
	}

	public TextGameObject getGameObject()
	{
		return gameObject;
	}

	public void setGameObject(TextGameObject gameObject)
	{
		this.gameObject = gameObject;
	}
}
