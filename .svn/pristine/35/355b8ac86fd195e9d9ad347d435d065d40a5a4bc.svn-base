<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>Identify Server</display-name>
	<listener>
		<listener-class>com.doteyplay.ServiceContext</listener-class>
	</listener>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.doteyplay.action.LoginAction</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>save</servlet-name>
		<servlet-class>com.doteyplay.action.SaveAction</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>save</servlet-name>
		<url-pattern>/save</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>quick</servlet-name>
		<servlet-class>com.doteyplay.action.QuickLoginAction</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>quick</servlet-name>
		<url-pattern>/quick</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>bind</servlet-name>
		<servlet-class>com.doteyplay.action.BindAccountAction</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>bind</servlet-name>
		<url-pattern>/bind</url-pattern>
	</servlet-mapping>
</web-app>
