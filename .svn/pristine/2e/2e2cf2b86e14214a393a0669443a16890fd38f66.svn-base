#include "BaseUI.h"
bool BaseUI::init(std::string fileName)
{
    layout=static_cast<Layout*>(cocostudio::GUIReader::getInstance()->widgetFromJsonFile(fileName.c_str()));
    this->addChild(layout);
	
    return true;
};
void BaseUI::onEnter()
{
    Layout::onEnter();
    this->initNetEvent();
}
void BaseUI::onExit()
{
    Layout::onExit();
}
